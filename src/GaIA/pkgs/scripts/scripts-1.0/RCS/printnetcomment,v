@#!/bin/sh
#
# Ad-hoc til dokumentation
#
# $Header$
#

OBJC="$FWDIR/conf/objects_5_0.C"

NETWORK=$1

case $NETWORK in
	"")	echo usage $0 network
		exit 1
		;;
esac

awk '	BEGIN { DOPRINT = 0 };

	$0 ~ /:type/		{ TYPE= $0 ; next }
	$0 ~ /:netmask/		{ NETMASK = $0 ; next }
	$0 ~ /:comments/	{ COMMENTS = $0 ; next }
	$0 ~ /:ipaddr .'$NETWORK'/ {

		IPADDR = $0;

		gsub(/:.* \(/, "", TYPE);
		gsub(/\)$/, "", TYPE);

		gsub(/:.* \(/, "", NETMASK);
		gsub(/\)$/, "", NETMASK);

		gsub(/:.* \(/, "", IPADDR);
		gsub(/\)$/, "", IPADDR);


		gsub(/:comments \("/, "", COMMENTS);
		gsub(/"\)$/, "", COMMENTS);

		print "type = " TYPE
		print "ipad = " IPADDR
		print "nmas = " NETMASK
		print "cmnt = " COMMENTS

		print "";

	}

' $OBJC

exit 0


