PKG="unbound_dns"
VER="1.4.22"
NAME="unbound"
VENDOR="NLnet Labs"
SRC="unbound-1.4.22"
ROOT="/home/unbound"
CONFIGURE="configure --prefix=/home/unbound --mandir=/home/unbound/share/man --bindir=/home/unbound --sbindir=/home/unbound/bin --with-ldns=/home/unbound/"
BUILD="make"
INSTALL="make install"
INSTALL_MORE="/bin/sh install_more.sh"
RCSTART=""
RCSTOP=""
RCSTARTDIRS=""
RCSTOPDIRS=""
MAKECLEAN="make distclean"
DELEFILES="*.bak"
