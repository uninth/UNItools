.br
	\fBapl data:\fR
.br
	LDNS_RDF_TYPE_APL,
.br
	\fBb32 string:\fR
.br
	LDNS_RDF_TYPE_B32_EXT,
.br
	\fBb64 string:\fR
.br
	LDNS_RDF_TYPE_B64,
.br
	\fBhex string:\fR
.br
	LDNS_RDF_TYPE_HEX,
.br
	\fBnsec type codes:\fR
.br
	LDNS_RDF_TYPE_NSEC,
.br
	\fBa RR type:\fR
.br
	LDNS_RDF_TYPE_TYPE,
.br
	\fBa class:\fR
.br
	LDNS_RDF_TYPE_CLASS,
.br
	\fBcertificate algorithm:\fR
.br
	LDNS_RDF_TYPE_CERT_ALG,
.br
