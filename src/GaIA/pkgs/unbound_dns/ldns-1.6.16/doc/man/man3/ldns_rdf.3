.br
	\fBperiod:\fR
.br
	LDNS_RDF_TYPE_PERIOD,
.br
	\fBtsig time 48 bits:\fR
.br
	LDNS_RDF_TYPE_TSIGTIME,
.br
	LDNS_RDF_TYPE_TSIG,
